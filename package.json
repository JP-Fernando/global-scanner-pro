{
  "name": "global-scanner-pro",
  "version": "0.0.6",
  "description": "Sistema cuantitativo profesional para anÃ¡lisis multi-factor de mercados globales",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc",
    "build:client": "vite build && node scripts/inject-vite-assets.js",
    "build:client:vite": "vite build",
    "preview": "vite preview --port 4173",
    "typecheck": "tsc --noEmit",
    "start": "NODE_ENV=production node dist/server.js",
    "data:enrich": "node scripts/enrich_universes.js",
    "dev": "tsx --watch server.js",
    "dev:server": "tsx --watch server.js",
    "dev:vite": "vite",
    "test": "vitest run",
    "test:unit": "vitest run --testPathPattern=unit",
    "test:integration": "vitest run --testPathPattern=integration",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:legacy": "node -e \"import('./src/tests/tests.js').then(m => m.runAllTests())\"",
    "test:api": "curl http://localhost:3000/api/run-tests",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:report": "playwright show-report",
    "test:bench": "vitest bench",
    "test:bench:json": "vitest bench --outputJson src/tests/performance/results/bench-results.json",
    "test:load": "node src/tests/performance/load/health-endpoint.load.js && node src/tests/performance/load/rate-limiting.load.js",
    "test:load:health": "node src/tests/performance/load/health-endpoint.load.js",
    "test:load:yahoo": "node src/tests/performance/load/yahoo-endpoint.load.js",
    "test:load:ratelimit": "node src/tests/performance/load/rate-limiting.load.js",
    "test:lighthouse": "node src/tests/performance/lighthouse/lighthouse-runner.js",
    "test:perf": "npm run test:bench && npm run test:load",
    "test:perf:all": "npm run test:bench && npm run test:load && npm run test:lighthouse",
    "perf:baseline": "vitest bench --outputJson src/tests/performance/results/bench-baseline.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "docker:build": "docker build -t global-scanner-pro .",
    "docker:run": "docker run --rm -p 3000:3000 --env-file .env global-scanner-pro",
    "docker:up": "docker-compose up",
    "docker:up:build": "docker-compose up --build",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f app"
  },
  "keywords": [
    "quantitative",
    "trading",
    "scanner",
    "technical-analysis",
    "stock-market",
    "finance",
    "momentum",
    "trend-following"
  ],
  "author": "JP-Fernando",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^10.34.0",
    "@sentry/profiling-node": "^10.34.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.22.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.2",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "prom-client": "^15.1.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.19.0",
    "xlsx": "^0.18.5",
    "yahoo-finance2": "^3.10.2",
    "zod": "^4.3.5"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JP-Fernando/global-scanner-pro.git"
  },
  "bugs": {
    "url": "https://github.com/JP-Fernando/global-scanner-pro/issues"
  },
  "homepage": "https://github.com/JP-Fernando/global-scanner-pro#readme",
  "lint-staged": {
    "*.{ts,js}": "eslint --fix"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.1",
    "@eslint/js": "^9.39.2",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.58.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "autocannon": "^8.0.0",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
